#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/proc/cygdrive/c/Users/rober/OneDrive/Documents/GitHub/apstat-chain/node_modules/.pnpm/peer@1.0.2_supports-color@5.5.0/node_modules/peer/dist/bin/node_modules:/proc/cygdrive/c/Users/rober/OneDrive/Documents/GitHub/apstat-chain/node_modules/.pnpm/peer@1.0.2_supports-color@5.5.0/node_modules/peer/dist/node_modules:/proc/cygdrive/c/Users/rober/OneDrive/Documents/GitHub/apstat-chain/node_modules/.pnpm/peer@1.0.2_supports-color@5.5.0/node_modules/peer/node_modules:/proc/cygdrive/c/Users/rober/OneDrive/Documents/GitHub/apstat-chain/node_modules/.pnpm/peer@1.0.2_supports-color@5.5.0/node_modules:/proc/cygdrive/c/Users/rober/OneDrive/Documents/GitHub/apstat-chain/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/proc/cygdrive/c/Users/rober/OneDrive/Documents/GitHub/apstat-chain/node_modules/.pnpm/peer@1.0.2_supports-color@5.5.0/node_modules/peer/dist/bin/node_modules:/proc/cygdrive/c/Users/rober/OneDrive/Documents/GitHub/apstat-chain/node_modules/.pnpm/peer@1.0.2_supports-color@5.5.0/node_modules/peer/dist/node_modules:/proc/cygdrive/c/Users/rober/OneDrive/Documents/GitHub/apstat-chain/node_modules/.pnpm/peer@1.0.2_supports-color@5.5.0/node_modules/peer/node_modules:/proc/cygdrive/c/Users/rober/OneDrive/Documents/GitHub/apstat-chain/node_modules/.pnpm/peer@1.0.2_supports-color@5.5.0/node_modules:/proc/cygdrive/c/Users/rober/OneDrive/Documents/GitHub/apstat-chain/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../peer/dist/bin/peerjs.js" "$@"
else
  exec node  "$basedir/../peer/dist/bin/peerjs.js" "$@"
fi
