#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/proc/cygdrive/c/Users/rober/OneDrive/Documents/GitHub/apstat-chain/packages/p2p/node_modules/esbuild/bin/node_modules:/proc/cygdrive/c/Users/rober/OneDrive/Documents/GitHub/apstat-chain/packages/p2p/node_modules/esbuild/node_modules:/proc/cygdrive/c/Users/rober/OneDrive/Documents/GitHub/apstat-chain/packages/p2p/node_modules:/proc/cygdrive/c/Users/rober/OneDrive/Documents/GitHub/apstat-chain/packages/node_modules:/proc/cygdrive/c/Users/rober/OneDrive/Documents/GitHub/apstat-chain/node_modules:/proc/cygdrive/c/Users/rober/OneDrive/Documents/GitHub/node_modules:/proc/cygdrive/c/Users/rober/OneDrive/Documents/node_modules:/proc/cygdrive/c/Users/rober/OneDrive/node_modules:/proc/cygdrive/c/Users/rober/node_modules:/proc/cygdrive/c/Users/node_modules:/proc/cygdrive/c/node_modules:/proc/cygdrive/c/Users/rober/OneDrive/Documents/GitHub/apstat-chain/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/proc/cygdrive/c/Users/rober/OneDrive/Documents/GitHub/apstat-chain/packages/p2p/node_modules/esbuild/bin/node_modules:/proc/cygdrive/c/Users/rober/OneDrive/Documents/GitHub/apstat-chain/packages/p2p/node_modules/esbuild/node_modules:/proc/cygdrive/c/Users/rober/OneDrive/Documents/GitHub/apstat-chain/packages/p2p/node_modules:/proc/cygdrive/c/Users/rober/OneDrive/Documents/GitHub/apstat-chain/packages/node_modules:/proc/cygdrive/c/Users/rober/OneDrive/Documents/GitHub/apstat-chain/node_modules:/proc/cygdrive/c/Users/rober/OneDrive/Documents/GitHub/node_modules:/proc/cygdrive/c/Users/rober/OneDrive/Documents/node_modules:/proc/cygdrive/c/Users/rober/OneDrive/node_modules:/proc/cygdrive/c/Users/rober/node_modules:/proc/cygdrive/c/Users/node_modules:/proc/cygdrive/c/node_modules:/proc/cygdrive/c/Users/rober/OneDrive/Documents/GitHub/apstat-chain/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../esbuild/bin/esbuild" "$@"
else
  exec node  "$basedir/../esbuild/bin/esbuild" "$@"
fi
